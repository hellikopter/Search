<Rules>
  <Rule>
    <Label>{1}-{2} Session</Label>
    <Pattern><![CDATA[publications.parliament.uk/pa/cm/cmregmem/.+(\d{2})(\d{2}).htm]]></Pattern>
    <Pattern><![CDATA[publications.parliament.uk/pa/(?:cm|ld)\d{2}(\d{2})(\d{2})]]></Pattern>
    <Pattern><![CDATA[parliament.uk/edm/\d{2}(\d{2})-(\d{2})]]></Pattern>
  </Rule>

  <Rule>
    <Label>{1}-{2}-{3}</Label>
    <Pattern><![CDATA[publications.parliament.uk/pa/.+/.+/(\d{2})(\d{2})(\d{2})]]></Pattern>
    <Pattern><![CDATA[archives.blog.parliament.uk/\d{2}(?<3>\d{2})/(?<2>\d{2})/(?<1>\d{2})]]></Pattern>
    <Pattern><![CDATA[hansard.parliament.uk/.+/\d{2}(?<3>\d{2})-(?<2>\d{2})-(?<1>\d{2})]]></Pattern>
  </Rule>

